{
  "name": "testlib",
  "version": "1.0.0",
  "description": "library setup for npm",
  "main": "./src/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist --ignore *.test.js  src",
    "lint": "eslint --debug  src",
    "test": "nyc mocha src/index.test.js",
    "watch:test": "mocha --watch",
    "validate": "npm-run-all --parallel lint test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougc333/testLIB.git"
  },
  "keywords": [
    "npm",
    "library",
    "development"
  ],
  "author": "doug chang <dougc@stanfordalumni.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougc333/testLIB/issues"
  },
  "homepage": "https://github.com/dougc333/testLIB#readme",
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.23.3",
    "babel-cli": "6.11.4",
    "babel-plugin-istanbul": "1.0.3",
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "eslint": "3.2.0",
    "ghooks": "1.3.2",
    "mocha": "3.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rimraf": "5.0.5"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 10,
    "statements": 10,
    "reporter": [
      "text",
      "lcov"
    ],
    "instrument": false,
    "sourceMap": false,
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "precommit": "npm run validate"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins":[
     "istanbul" 
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "babel-preset-es2015": "6.9.0",
    "unique-random-array": "1.0.0"
  }
}
