{
  "name": "testlib",
  "version": "1.0.0",
  "description": "library setup for npm",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint --debug  src",
    "test": "nyc mocha src/index.test.js",
    "watch:test": "mocha --watch",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougc333/testLIB.git"
  },
  "keywords": [
    "npm",
    "library",
    "development"
  ],
  "author": "doug chang <dougc@stanfordalumni.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougc333/testLIB/issues"
  },
  "homepage": "https://github.com/dougc333/testLIB#readme",
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "7.23.3",
    "chai": "3.5.0",
    "eslint": "3.2.0",
    "ghooks": "2.0.4",
    "mocha": "3.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 10,
    "statements": 10,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "precommit": "npm run validate"
    }
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  }
}
